/* Special grid styles
-------------------------------------------------- */
h1 {font-family: 'Lato', sans-serif; }
h2 {font-family: 'Cabin Condensed', sans-serif;}.white{color:#fff!important;}
.show-grid {
  margin-top: 10px;
  margin-bottom: 10px;
}
.show-grid [class*="span"] {
 
  text-align: left;
  -webkit-border-radius: 5px;
     -moz-border-radius: 5px;
          border-radius: 5px;
  min-height: 30px;
  line-height: 30px;
}
.show-grid:hover [class*="span"] {
  /*background: #ddd;*/
}
.show-grid .show-grid {
  margin-top: 0;
  margin-bottom: 0;
}
.show-grid .show-grid [class*="span"] {
  background-color: #ccc;
}


